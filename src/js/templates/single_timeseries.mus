<div>
  {{#showChange}}
    <p class="t-change change change-{{ latest.formatted_change_from_previous.trend }}">{{ latest.formatted_change_from_previous.change }} on previous {{ latest.period }}</p>
  {{/showChange}}
  {{#showSecondLatest}}
    <p class="t-second-most-recent second-most-recent">{{ secondLatest.formatted_value }} for previous {{ secondLatest.period }}</p>
  {{/showSecondLatest}}
</div>