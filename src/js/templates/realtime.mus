<div class="t-slide-realtime slide {{ departmentCode }}" data-module-slug={{ slug }}>
  <div class="content">
    <h2 class="dashboard-title">{{ dashboardTitle }}</h2>
    <h3 class="t-module-title module-title">{{ title }}</h3>
    <span class="t-main-figure main-figure{{^latestAvailable}} main-figure-no-data{{/latestAvailable}}" data-realtime="latest.formatted_value">{{ latest.formatted_value }}</span>
    <p class="data-url"><a class="link-dashboard" href="http://www.gov.uk/performance/{{ dashboardSlug }}#{{ slug }}">www.gov.uk/performance/{{ dashboardSlug }}</a></p>
  </div>
</div>